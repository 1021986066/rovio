<?xml version="1.0" ?>
<launch>
  <param name="use_sim_time" value="true" />
  <arg name="lidar" default="true"/>
  <node pkg="rosbag" type="play" name="player" output="screen" args="--clock --pause /home/nico/datasets/2019-02-26-voliroT/2019-02-26-15-13-03-easy.bag"/>


  <node pkg="tf" type="static_transform_publisher" name="world_vi" args="0 0 0 0 0 0 world vicon 100" />

  <include if="$(arg lidar)" file="$(find ouster_ros)/os1_points.launch">
  </include>
  <node if="$(arg lidar)" pkg="tf" type="static_transform_publisher" name="c_s" args="-0.056028336799999995 0.1291542 -0.0913737748 0.526637856695111 0.5061155357097586 0.4831075105838945 -0.4828113147247448 camera0 os1_sensor 100" />

  <node pkg="rovio" type="rovio_node" name="rovio" output="screen">
    <param name="filter_config" value="$(find rovio)/cfg/rovio_voliroT.info"/>
    <param name="camera0_config" value="$(find rovio)/cfg/voliroT_cam0.yaml"/>
    <param name="imu_topic_name" value="/imu0"/>
    <param name="cam0_topic_name" value="/cam0/image_raw"/>
  </node>

</launch>
